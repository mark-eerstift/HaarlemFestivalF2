@model IEnumerable<ProjectWebApplicatie.Models.Dance>

@{
    ViewBag.Title = "ViewDanceSales";
}

<h2>View Dance Sales</h2>


@Html.ActionLink("View All Sales", "ViewDanceSales") |
@Html.ActionLink("View Thursday Sales", "ViewDanceSalesByDate", new DateTime(2019, 07, 27)) |
@Html.ActionLink("View Friday Sales", "ViewDanceSalesByDate", new DateTime(2019, 07, 28)) |
@Html.ActionLink("View Saturday Sales", "ViewDanceSalesByDate", new DateTime(2019, 07, 29)) |





<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Locatie)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BeginTijd)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EindTijd)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TicketsTotaal)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TicketsVerkocht)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Session)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Artiest)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Locatie)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BeginTijd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EindTijd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TicketsTotaal)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TicketsVerkocht)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Session)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Artiest)
            </td>

        </tr>
    }

</table>
